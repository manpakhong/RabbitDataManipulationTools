package hksarg.swd.csss.csa.flowtest.models.dtos;

public class WhereClauseDto {
	private int numberOfKeyColumns;
	private String whereClauseStatement;
	public int getNumberOfKeyColumns() {
		return numberOfKeyColumns;
	}
	public void setNumberOfKeyColumns(int numberOfKeyColumns) {
		this.numberOfKeyColumns = numberOfKeyColumns;
	}
	public String getWhereClauseStatement() {
		return whereClauseStatement;
	}
	public void setWhereClauseStatement(String whereClauseStatement) {
		this.whereClauseStatement = whereClauseStatement;
	}
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("WhereClauseDto [numberOfKeyColumns=");
		builder.append(numberOfKeyColumns);
		builder.append(", whereClauseStatement=");
		builder.append(whereClauseStatement);
		builder.append("]");
		return builder.toString();
	}
	
	
}
