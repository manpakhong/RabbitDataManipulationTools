package hksarg.swd.csss.csa.flowtest.factories;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;

public class ScopeTestGlobalVariablesFactory {
	private final static Logger logger = LoggerFactory.getLogger(ScopeTestGlobalVariablesFactory.class);
	private final static String className = ScopeTestGlobalVariablesFactory.class.getName();
	private static boolean isUsed = false;
	private static String scopeName = "";
	private static String result = "";
	private static boolean isOk = false;

	public static String getClassname() {
		return className;
	}

	public static boolean getIsUsed() {
		return isUsed;
	}
	public static void setIsUsed(boolean isUsed) {
		ScopeTestGlobalVariablesFactory.isUsed = isUsed;
	}
	public static String getScopeName() {
		return scopeName;
	}
	public static void setScopeName(String scopeName) {
		ScopeTestGlobalVariablesFactory.scopeName = scopeName;
	}
	public static String getResult() {
		return result;
	}
	public static void setResult(String result) {
		ScopeTestGlobalVariablesFactory.result = result;
	}
	public static boolean getIsOk() {
		return isOk;
	}
	public static void setIsOk(boolean isOk) {
		ScopeTestGlobalVariablesFactory.isOk = isOk;
	}
	public Logger getLogger() {
		return logger;
	}
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("GlobalVariablesFactory [logger=");
		builder.append(logger);
		builder.append(", className=");
		builder.append(className);
		builder.append("]");
		return builder.toString();
	}
	
	
}
